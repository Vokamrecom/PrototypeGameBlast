{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es2015", "es2017"],
    "target": "es5",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "temp/",
    "allowJs": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*"]
    }
  },
  "include": [
    "assets/**/*"
  ],
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build"
  ]
}
